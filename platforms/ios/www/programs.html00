  <style type="text/css">
    * {
      padding: 0;
      margin: 0;
      border: 0;
    }

  
    #demo {
      position: absolute;
      top: 20px;
      left: 0;
      width: 100%;
      height: 400px;
      visibility: hidden;
       cursor: hand;
      cursor: -moz-grab;
      cursor: -webkit-grab;
      cursor: grab;
    }
 .list {
     
      top: 0px;
      left: 0;
      width: 80%;
      height: 300px;
 
      background: #457;
      cursor: hand;
      cursor: -moz-grab;
      cursor: -webkit-grab;
      cursor: grab;
    }

    #demo li.first {
      background: #577;
    }

    #demo li.middle {
      background: #1e8;
    }

    #demo li.last {
      background: #e8b;
    }

       #demo p {
      max-width: 600px;
      margin: 0 auto;
      padding: 0 30px;
      margin-bottom: 30px;
    }

    #demo p:first-child {
      margin-top: 30px;
    }

    #demo p:last-child {
      margin-bottom: 200px;
    }
	 .grad{
	 background: -webkit-linear-gradient(top, #601348 0%,#56224c 30%,#17115e 100%); /* Chrome10-25,Safari5.1-6 */
	 display:block;
	 height:100%;
	 }
  </style>
